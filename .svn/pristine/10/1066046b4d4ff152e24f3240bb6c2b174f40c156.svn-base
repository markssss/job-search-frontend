    <body>

        <div id="sitewrapper" class="site-wrapper">
            <div id="index" class="site-wrapper-inner">
                <div id="landingmain"></div>
                <div id="covercontainer" class="cover-container">
                    <div id="inner-cover" class="inner cover" style="padding-bottom: 150px;">
                        <div id="logo"  class="logo">
                            <span><b style="font-family: 'Roboto', sans-serif;">Job Search</b></span>
                        </div>
                        <div id="form" class="container-fluid">
                            <form class="form-inline" role="form" method="get" action="search" id="searchForm">
                                <div class="form-group">
                                    <input id="tags" type="text" placeholder="skills and keywords" class="form-control" >
                                </div>
                                <div class="form-group">
                                    <input id="loc" type="text" placeholder="city, state or zip" class="form-control">
                                </div>
                                <button id="searchBtn" type="submit" class="btn btn-danger">Search</button>
                            </form> 
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="results-nav"></div>
        <div id="results-div" class="results-div">
            <div class="col1">&nbsp;</div>
            <div id="results" class="results col2"></div>
        </div>
        <footer style="clear: both;">
            <div >
                <p>&copy USA 2014 </p>
            </div>
        </footer>
